{
  "name": "expo-with-realworld",
  "description":
    "expo-with-realworld for large scale app build in React-Native using expo sdk",
  "slug": "expo-with-realworld",
  "privacy": "public",
  "sdkVersion": "20.0.0",
  "version": "1.0.0",
  "orientation": "portrait",
  "primaryColor": "#cccccc",
  "icon": "./assets/icons/duckduckgo.png",
  "notification": {
    "icon": "./assets/icons/notification-icon.png",
    "color": "#000000"
  },
  "loading": {
    "icon": "./assets/icons/duckduckgo.png",
    "hideExponentText": false
  },
  "packagerOpts": {
    "assetExts": ["ttf"]
  },
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "package": "expo"
  }
}
